<h1>Editing frequency</h1>

<% form_for(@frequency) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :frequency %><br />
    <%= f.text_field :frequency %>
  </p>
  <p>
    <%= f.label :modulation %><br />
    <%= f.text_field :modulation %>
  </p>


  <p>
    <%= f.label :call %><br />
    <%= f.text_field :call %>
  </p>

  <p>
    <%= f.label :digimode %><br />
    <%= f.text_field :digimode %>
  </p>

  <p>
    <%= f.label :radio %><br />
    <%= f.text_field :radio %>
  </p>

  <p>
    <%= f.label :s_meter %><br />
    <%= f.text_field :s_meter %>
  </p>

  <p>
    <%= f.label :antenna %><br />
    <%= f.text_field :antenna %>
  </p>

  <p>
    <%= f.label :date %><br />
    <%= f.text_field :date %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @frequency %> |
<%= link_to 'Back', frequencies_path %>
